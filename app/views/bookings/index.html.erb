<%= render 'shared/navbar' %>
<div class="container">
 <% @bookings.each do |booking| %>
<div>
<%= booking.title %>
<%= booking.category %>
<%= link_to edit_booking_path(booking) do %>
<i class="fas fa-edit"></i>
<% end %>
<%= link_to booking_path(booking), method: :delete, data: { confirm: "Êtes-vous sûr?" } do %>
<i class="fas fa-trash"></i>
<% end %>
<% end %>
</div>
</div>


